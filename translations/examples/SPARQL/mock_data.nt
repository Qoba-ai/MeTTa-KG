@prefix ex: <http://example.org/> .
@prefix prop: <http://example.org/property/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# Users
ex:user1 rdf:type ex:User ;
         prop:name "Alice" ;
         prop:user_id "1" .

ex:user2 rdf:type ex:User ;
         prop:name "Bob" ;
         prop:user_id "2" .

ex:user3 rdf:type ex:User ;
         prop:name "Charlie" ;
         prop:user_id "3" .

# Comments
ex:comment101 rdf:type ex:Comment ;
              prop:comment_id "101" ;
              prop:user_id "1" ;
              prop:comment_votes -1.

ex:comment102 rdf:type ex:Comment ;
              prop:comment_id "102" ;
              prop:user_id "2" ;
              prop:comment_votes -1.

ex:comment103 rdf:type ex:Comment ;
              prop:comment_id "103" ;
              prop:user_id "3" ;
              prop:comment_votes 1.

ex:comment104 rdf:type ex:Comment ;
              prop:comment_id "104" ;
              prop:user_id "3" ;
              prop:comment_votes 1.

# Votes
ex:vote201 rdf:type ex:Vote ;
           prop:comment_id "101" ;
           prop:vote_type "upvote" .

ex:vote202 rdf:type ex:Vote ;
           prop:comment_id "101" ;
           prop:vote_type "downvote" .

ex:vote206 rdf:type ex:Vote ;
           prop:comment_id "101" ;
           prop:vote_type "downvote" .

ex:vote203 rdf:type ex:Vote ;
           prop:comment_id "102" ;
           prop:vote_type "downvote" .

ex:vote204 rdf:type ex:Vote ;
           prop:comment_id "103" ;
           prop:vote_type "upvote" .

ex:vote205 rdf:type ex:Vote ;
           prop:comment_id "104" ;
           prop:vote_type "upvote" .


# Proposals

ex:proposal1 rdf:type ex:Proposal ;
             prop:proposer_id "1" ;
             prop:proposal_id "1" ;
             prop:round_id 1 ;
             prop:awarded true .

ex:proposal2 rdf:type ex:Proposal ;
             prop:proposer_id "1" ;
             prop:proposal_id "2" ;
             prop:round_id 1 ;
             prop:awarded false .

ex:proposal3 rdf:type ex:Proposal ;
             prop:proposer_id "1" ;
             prop:proposal_id "3" ;
             prop:round_id 2 ;
             prop:awarded true .

ex:proposal4 rdf:type ex:Proposal ;
             prop:proposer_id "2" ;
             prop:proposal_id "4" ;
             prop:round_id 2 ;
             prop:awarded true .

# Reviews
ex:review301 rdf:type ex:Review ;
             prop:review_id "301" ;
             prop:proposal_id "1" ;
             prop:review_type "community" ;
             prop:overall_rating 4.

ex:review302 rdf:type ex:Review ;
             prop:review_id "302" ;
             prop:proposal_id "1" ;
             prop:review_type "expert" ;
             prop:overall_rating 2.

ex:review303 rdf:type ex:Review ;
             prop:review_id "303" ;
             prop:proposal_id "1" ;
             prop:review_type "expert" ;
             prop:overall_rating 3.  # Second expert review for proposal 1

ex:review304 rdf:type ex:Review ;
             prop:review_id "304" ;
             prop:proposal_id "2" ;
             prop:review_type "community" ;
             prop:overall_rating 5.

ex:review305 rdf:type ex:Review ;
             prop:review_id "305" ;
             prop:proposal_id "2" ;
             prop:review_type "community" ;
             prop:overall_rating 4.  # Second community review for proposal 2

ex:review306 rdf:type ex:Review ;
             prop:review_id "306" ;
             prop:proposal_id "2" ;
             prop:review_type "expert" ;
             prop:overall_rating 3.

ex:review307 rdf:type ex:Review ;
             prop:review_id "307" ;
             prop:proposal_id "3" ;
             prop:review_type "community" ;
             prop:overall_rating 1.

ex:review308 rdf:type ex:Review ;
